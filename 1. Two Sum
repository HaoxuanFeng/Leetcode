class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        index1 = 0
        index2 = 0
        for x in nums:
            for y in nums:
                if index2 == 0:
                    index2 = index2 + 1
                    continue
                elif x+y == target:
                    return index1,index2
                index2 = index2 + 1
            index1 = index1 + 1
            index2 = 0
